meta {
  name: Remove from collection
  type: http
  seq: 8
}

post {
  url: {{assetsUrl}}/services/removeRelation
  body: multipartForm
  auth: inherit
}

body:multipart-form {
  relationIds: {{tempTestRelationId}}
}

assert {
  res.status: eq 200
  res.body: isJson
  res.body.processedCount: eq 1
}

settings {
  encodeUrl: true
  timeout: 0
}

docs {
  WoodWing Helpcenter: [Assets Server REST API - remove relation](https://helpcenter.woodwing.com/hc/en-us/articles/360041851332-Assets-Server-REST-API-remove-relation)
}
